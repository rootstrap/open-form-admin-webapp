{
  "compilerOptions": {
    "baseUrl": "./",
    "paths": {
      "*": ["src/*"],
      "actions/*": ["src/actions/*"],
      "api/*": ["src/api/*"],
      "components/*": ["src/components/*"],
      "constants/*": ["src/constants/*"],
      "hooks/*": ["src/hooks/*"],
      "locales/*": ["src/locales/*"],
      "pages/*": ["src/pages/*"],
      "reducers/*": ["src/reducers/*"],
      "selectors/*": ["src/selectors/*"],
      "services/*": ["src/services/*"],
      "store/*": ["src/store/*"],
      "styles/*": ["src/styles/*"],
      "utils/*": ["src/utils/*"]
    }
  },
  "exclude": ["node_modules", "dist"]
}
